                 foaf:member sdaperson:JensLehmann, sdaperson:CalvinHidermanns ;
                 foaf:homepage <http://www.projekt-geiser.de> ;
                 foaf:logo <http://sda.cs.uni-bonn.de/wp-content/uploads/2016/02/logo-BigDataEurope.png> ;
