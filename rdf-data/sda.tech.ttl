        dct:hasFormat <http://svn.aksw.org/papers/2016/ONTOBRAS_LDWPO/public.pdf>  ;
        dct:modified "rautenberg-2016"  .

sdapublication:rautenberg-2015 a bibtex:InProceedings, dct:BibliographicResource ;
        dct:title "LODFlow -- a Workflow Management System for Linked Data Processing"  ;
        dct:creator  sdaperson:SandroRautenberg,  sdaperson:IvanErmilov,  sdaperson:EdgardMarx,  sdaperson:SorenAuer,  sdaperson:AxelCyrilleNgomoNgonga  ;
        dct:isPartOf "SEMANTiCS 2015"  ;
        bibtex:year "2015"  ;
        dct:abstract "The extraction and maintenance of Linked Data datasets is a cumbersome, time-consuming and resource-intensive activity The cost for producing Linked Data can be reduced by a workflow management system, which describes plans to systematically support the lifecycle of RDF datasets We present the LODFlow Linked Data Workflow Management System, which provides an environment for planning, executing, reusing, and documenting Linked Data workflows The LODFlow approach is based on a comprehensive knowledge model for describing the workflows and a workflow execution engine supporting systematic workflow execution, reporting, and exception handling The environment was evaluated in a large-scale real-world use case As result, LODFlow supports Linked Data engineers to systematically plan, execute and assess Linked Data production and maintenance workflows, thus improving efficiency, ease-of-use, reproducibility, reuseability and provenance"  ;
        sda:bdsk-url-1 "http://svn.aksw.org/papers/2015/SEMANTICS_LDWPO/public.pdf"  ;
        bibtex:keywords "2015 sake ngonga auer iermilov marx simba groupaksw sysrelevantForinfai sysrelevantForbis SIMBA rautenberg sysrelevantForgeoknow geoknow"  ;
        sda:owner "ivan"  ;
        sda:timestamp "2015.08.12"  ;
        dct:hasFormat <http://svn.aksw.org/papers/2015/SEMANTICS_LDWPO/public.pdf>  ;
        dct:modified "rautenberg-2015"  .

