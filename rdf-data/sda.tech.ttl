        dct:hasFormat <http://svn.aksw.org/papers/2017/ESWC_WOMBAT/public.pdf>  ;
        dct:modified "WOMBAT_2017"  .

sdapublication:DEER_2015 a bibtex:InProceedings, dct:BibliographicResource ;
        dct:title "Automating RDF Dataset Transformation and Enrichment"  ;
