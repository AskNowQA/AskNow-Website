        sda:owner "sherif"  ;
        sda:timestamp "2014.03.17"  ;
        dct:hasFormat <http://svn.aksw.org/papers/2013/ISWC_CHIMERA/public.pdf>  ;
        dct:modified "colibriESWC14"  .

