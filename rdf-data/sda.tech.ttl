        sda:owner "mohamed"  ;
        sda:timestamp "2012.04.13"  ;
        dct:hasFormat <http://svn.aksw.org/papers/2011/DBpedia_Live/public.pdf>  ;
        dct:modified "dbpedia_live_2012"  .

