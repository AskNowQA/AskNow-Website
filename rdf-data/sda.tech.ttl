sdapublication:riechert-t-2010-iswc a bibtex:InProceedings, dct:BibliographicResource ;
        dct:title "Knowledge Engineering for Historians on the Example of the Catalogus Professorum Lipsiensis"  ;
        dct:creator  sdaperson:ThomasRiechert,  sdaperson:UlfMorgenstern,  sdaperson:SorenAuer,  sdaperson:SebastianTramp,  sdaperson:MichaelMartin  ;
        dct:isPartOf "Proceedings of the 9th International Semantic Web Conference ISWC2010"  ;
        bibtex:year "2010"  ;
        address:localityName "Shanghai  China"  ;
        bibo:editor "Patel-Schneider, Peter F and Pan, Yue and Hitzler, Pascal and Mika, Peter and Zhang, Lei and Pan, Jeff Z and Horrocks, Ian and Glimm, Birte"  ;
        bibtex:editor "Patel-Schneider, Peter F and Pan, Yue and Hitzler, Pascal and Mika, Peter and Zhang, Lei and Pan, Jeff Z and Horrocks, Ian and Glimm, Birte"  ;
        bibo:pageStart "225"  ;
