sdapublication:tramp-s-2014-thesis a bibo:Thesis, sdatech:completedThesis, dct:BibliographicResource ;
        dct:title "Distributed Semantic Social Networks: Architecture, Protocols and Applications"  ;
        dct:creator  sdaperson:SebastianTramp  ;
        bibtex:school "University of Leipzig"  ;
        bibtex:year "2014"  ;
        address:localityName "Leipzig, Germany"  ;
